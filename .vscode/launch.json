{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Alchemist Service Local",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/server/main.go",
      "env": {
        "DB_HOST": "localhost",
        "DB_PORT": "5432",
        "DB_USER": "postgres",
        "DB_PASSWORD": "secret",
        "DB_NAME": "alchemist",
        "DB_SSL_MODE": "disable",
        "ACCESS_TOKEN_SECRET": "access_secret",
        "ACCESS_TOKEN_DURATION_SECS": "60",
        "REFRESH_TOKEN_SECRET": "refresh_secret",
        "REFRESH_TOKEN_DURATION_SECS": "600",
        "SESSION_COOKIE_NAME": "user_session",
        "SESSION_COOKIE_MAX_AGE_SECS": "600",
        "SESSION_COOKIE_PATH": "/",
        "SESSION_COOKIE_DOMAIN": "localhost",
        "SESSION_COOKIE_SECURE": "false",
        "SESSION_COOKIE_HTTP_ONLY": "true"
      }
    }
  ]
}
